{
    "transforms": {
        "train": [
            {"name": "RandAugment", "n": 3, "m": 5, "fillcolor": [125, 123, 114]},
            {"name": "Cutout", "ratio": 0.5, "fillcolor": [125, 123, 114]},
            {"name": "RandomCrop", "size": 32, "padding": 4, "padding_mode": "reflect"},
            {"name": "RandomHorizontalFlip"},
            {"name": "ToTensor"},
            {"name": "Normalize", "dataset": "CIFAR10"}
        ],
        "val": [
            {"name": "ToTensor"},
            {"name": "Normalize", "dataset": "CIFAR10"}
        ]
    },
    "model": {
        "backbone": {
            "src": "torchssl",
            "name": "wide_resnet28_2",
            "num_classes": 10    
        },
        "momentum": 0.99
    },
    "dataset": {
        "name": "cifar10",
        "batch_sizes": {
            "train": 128,
            "val": 128
        }
    },
    "optimizer": {
        "name": "SGD",
        "lr": 0.1,
        "momentum": 0.9,
        "weight_decay": 5e-4,
        "nesterov": true
    },
    "scheduler": {
        "name": "CosineAnnealingLR",
        "warmup": 5,
        "T": 300
    }
}