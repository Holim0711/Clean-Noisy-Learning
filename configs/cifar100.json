{
    "method": "noisy-flexmatch",
    "transform": {
        "weak": [
            {"name": "RandomHorizontalFlip"},
            {"name": "RandomCrop", "size": 32, "padding": 4, "padding_mode": "reflect"},
            {"name": "ToTensor"},
            {"name": "Normalize", "mean": "cifar100", "std": "cifar100"}
        ],
        "strong": [
            {"name": "RandomHorizontalFlip"},
            {"name": "RandomCrop", "size": 32, "padding": 4, "padding_mode": "reflect"},
            {"name": "AllRandAugment", "num_ops": 2, "fill": "cifar100"},
            {"name": "Cutout", "ratio": 0.5, "fill": "cifar100"},
            {"name": "ToTensor"},
            {"name": "Normalize", "mean": "cifar100", "std": "cifar100"}
        ],
        "val": [
            {"name": "ToTensor"},
            {"name": "Normalize", "mean": "cifar100", "std": "cifar100"}
        ]
    },
    "model": {
        "backbone": {
            "src": "weaver",
            "name": "preact_resnet18",
            "num_classes": 100
        },
        "loss_u": {
            "temperature": 0.5,
            "threshold": 0.95
        },
        "ema": 0.999
    },
    "dataset": {
        "name": "CIFAR100",
        "root": "data/CIFAR100",
        "num_clean": 1000,
        "noise_type": "symmetric",
        "noise_level": 0.2,
        "random_seed": 0,
        "batch_size": {
            "clean": 64,
            "noisy": 448
        }
    },
    "optimizer": {
        "name": "SGD",
        "lr": 0.03,
        "momentum": 0.9,
        "weight_decay": 0.001,
        "nesterov": true
    },
    "scheduler": {
        "name": "HalfCosineAnnealingLR",
        "T_max": 10700
    }
}