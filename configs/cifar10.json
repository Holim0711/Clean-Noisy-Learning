{
    "method": "noisy-flexmatch",
    "transform": {
        "weak": [
            {"name": "RandomHorizontalFlip"},
            {"name": "RandomCrop", "size": 32, "padding": 4, "padding_mode": "reflect"},
            {"name": "ToTensor"},
            {"name": "Normalize", "dataset": "CIFAR10"}
        ],
        "str": [
            {"name": "RandomHorizontalFlip"},
            {"name": "RandomCrop", "size": 32, "padding": 4, "padding_mode": "reflect"},
            {"name": "RandAugmentUDA", "n": 2, "fillcolor": "CIFAR10"},
            {"name": "Cutout", "ratio": 0.5, "fillcolor": "CIFAR10"},
            {"name": "ToTensor"},
            {"name": "Normalize", "dataset": "CIFAR10"}
        ],
        "val": [
            {"name": "ToTensor"},
            {"name": "Normalize", "dataset": "CIFAR10"}
        ]
    },
    "model": {
        "backbone": {
            "src": "weaver",
            "name": "preact_resnet18",
            "num_classes": 10
        },
        "loss_u": {
            "temperature": 0.5,
            "threshold": 0.95
        },
        "ema": 0.999,
        "lrelu": 0.1
    },
    "dataset": {
        "name": "CIFAR10",
        "num_clean": 100,
        "noise_type": "symmetric",
        "noise_ratio": 0.2,
        "random_seed": 0,
        "batch_sizes": {
            "clean": 64,
            "noisy": 448,
            "val": 512
        }
    },
    "optimizer": {
        "name": "SGD",
        "lr": 0.03,
        "momentum": 0.9,
        "weight_decay": 0.0005,
        "nesterov": true
    },
    "scheduler": {
        "name": "HalfCosineAnnealingLR",
        "T_max": 10700
    }
}